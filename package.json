    "vuex": "3.0.1",
    "vuex-typescript": "3.0.2"
  },
  "devDependencies": {
    "@types/date-fns": "2.6.0",
    "@types/graphql": "14.0.1",
    "@types/marked": "0.4.2",
    "@vue/cli-plugin-babel": "3.2.0",
    "@vue/cli-plugin-typescript": "3.2.0",
    "@vue/cli-service": "3.2.0",
    "apollo": "2.1.6",
    "babel-plugin-transform-class-properties": "6.24.1",
    "cross-env": "5.2.0",
    "electron": "3.0.10",
    "electron-builder": "20.38.2",
    "graphql-tag": "2.10.0",
    "lint-staged": "8.1.0",
    "node-sass": "4.10.0",
    "sass-loader": "7.1.0",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.2.1",
    "vue-cli-plugin-apollo": "0.18.1",
    "vue-cli-plugin-electron-builder": "1.0.0-rc.9",
    "vue-svg-loader": "0.11.0",
    "vue-template-compiler": "2.5.17",
    "webpack": "4.26.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "license": "AGPL-3.0-only",
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "background.js",
  "private": true,
  "repository": {
    "type": "github",
    "url": "https://github.com/BeeeQueue/yuna"
  },
  "resolutions": {
    "@types/graphql": "14.0.1",
    "graphql": "14.0.2"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production vue-cli-service electron:build",
    "graphql:get-schema": "yarn apollo schema:download graphql.schema.json",
    "graphql:types": "yarn apollo codegen:generate --addTypename --outputFlat --target=typescript --globalTypesFile=src/graphql-types.ts src/graphql",
    "lint": "vue-cli-service lint",
    "postinstall": "yarn electron-builder install-app-deps",
    "serve": "vue-cli-service electron:serve -c.productName=yuna-dev"
  }
}
