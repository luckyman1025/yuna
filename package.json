    "@types/electron-store": "1.3.0",
    "@types/hls.js": "0.10.2",
    "@types/semver": "5.5.0",
    "@types/superagent": "3.8.4",
    "@types/uuid": "3.4.4",
    "@types/webpack-env": "1.13.6",
    "apollo-boost": "0.1.22",
    "date-fns": "2.0.0-alpha.25",
    "electron-debug": "2.0.0",
    "electron-log": "2.2.17",
    "electron-store": "2.0.0",
    "electron-updater": "4.0.5",
    "electron-util": "0.10.2",
    "fuse.js": "3.3.0",
    "hls.js": "0.11.0",
    "marked": "0.5.2",
    "normalize.css": "8.0.1",
    "rambdax": "0.23.0",
    "raw-loader": "0.5.1",
    "superagent": "4.0.0",
    "svg-country-flags": "1.2.6",
    "ts-key-enum": "2.0.0",
    "uuid": "3.3.2",
    "v-tooltip": "2.0.0-rc.33",
    "vue": "2.5.17",
    "vue-apollo": "3.0.0-beta.26",
    "vue-class-component": "6.3.2",
    "vue-property-decorator": "7.2.0",
